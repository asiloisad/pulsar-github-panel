'.workspace':
  'ctrl-8': 'github-panel:toggle-github-panel-tab-focus'
  'ctrl-shift-8': 'github-panel:toggle-github-panel-tab'
  'alt-g r': 'github-panel:open-reviews-tab'

'.github-panel-Dialog':
  'tab': 'core:focus-next'
  'shift-tab': 'core:focus-previous'

'.github-panel-Dialog input':
  'enter': 'core:confirm'

'body .github-panel-TabbableWrapper':
  'down': 'github-panel:selectbox-down'
  'up': 'github-panel:selectbox-up'
  'enter': 'github-panel:selectbox-enter'
  'tab': 'github-panel:selectbox-tab'
  'backspace': 'github-panel:selectbox-backspace'
  'pageup': 'github-panel:selectbox-pageup'
  'pagedown': 'github-panel:selectbox-pagedown'
  'end': 'github-panel:selectbox-end'
  'home': 'github-panel:selectbox-home'
  'delete': 'github-panel:selectbox-delete'
  'escape': 'github-panel:selectbox-escape'

'.platform-darwin .github-panel-Reviews':
  'cmd-=': 'github-panel:more-context'
  'cmd--': 'github-panel:less-context'
'.platform-win32 .github-panel-Reviews':
  'ctrl-=': 'github-panel:more-context'
  'ctrl--': 'github-panel:less-context'
'.platform-linux .github-panel-Reviews':
  'ctrl-=': 'github-panel:more-context'
  'ctrl--': 'github-panel:less-context'

'.platform-darwin .github-panel-Reviews atom-text-editor':
  'cmd-enter': 'github-panel:submit-comment'
'.platform-win32 .github-panel-Reviews atom-text-editor':
  'ctrl-enter': 'github-panel:submit-comment'
'.platform-linux .github-panel-Reviews atom-text-editor':
  'ctrl-enter': 'github-panel:submit-comment'
